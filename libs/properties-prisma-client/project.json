{
  "name": "properties-prisma-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/properties-prisma-client/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project properties-prisma-client --web",

  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "libs/properties-prisma-client"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "libs/properties-prisma-client"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/properties-prisma-client"
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node libs/properties-prisma-client/prisma/seed/seed.ts"
      }
    }
  }
}
